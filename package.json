    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/test-utils": "^2.0.2",
    "eslint": "^8.23.0",
    "eslint-plugin-vue": "^9.4.0",
    "gh-pages": "^4.0.0",
    "jsdom": "^20.0.0",
    "prettier": "^2.7.1",
    "sass": "^1.54.6",
    "typescript": "^4.6.4",
    "vite": "^3.0.7",
    "vitest": "^0.22.1",
    "vue-tsc": "^0.39.5"
